#!/usr/bin/env bash
set -Eeuo pipefail

printf "The command-line client ships alongside the daemon. Usage information for all commands, including sub-commands, is available via the --help flag.\n\n<style>pre { background: inherit !important; word-wrap: inherit !important;  white-space: pre-wrap !important; }</style>\n%s" "$(textile docs)" > ./docs/develop/clients/command-line.md

mkdocs build